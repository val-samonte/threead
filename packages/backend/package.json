{
  "name": "@threead/backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "db:migrate": "wrangler d1 execute threead-db --file=scripts/migrate.sql",
    "db:migrate:remote": "wrangler d1 execute threead-db --remote --file=scripts/migrate.sql",
    "db:create": "wrangler d1 create threead-db"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20241106.0",
    "@modelcontextprotocol/sdk": "^1.20.2",
    "@solana/kit": "^5.0.0",
    "@threead/shared": "*",
    "bs58": "^6.0.0",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@solana-program/system": "^0.9.0",
    "@solana/programs": "^5.0.0",
    "@solana/spl-token": "^0.4.14",
    "@solana/transactions": "^5.0.0",
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^2.1.9",
    "tsx": "^4.20.6",
    "typescript": "^5.3.0",
    "vitest": "^2.1.9",
    "wrangler": "^4.45.3"
  }
}
